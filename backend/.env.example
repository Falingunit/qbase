# Copy this file to .env and fill in values

# Environment
NODE_ENV=production
PORT=3000

# Auth
# Use a long, random string in production
JWT_SECRET=change-me-strong-and-random
JWT_EXPIRES_IN=7d

# Data source for assignment JSON (location hosting the data/ folder)
ASSETS_BASE=https://falingunit.github.io/qbase

# Dev helper (do NOT enable in production)
# 1 = allow all CORS origins temporarily for local testing
ALLOW_ALL_ORIGINS=0

